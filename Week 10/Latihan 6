Array1 = [[4, 6],
           [1, 7]]

Array2 = [[2, 3],
           [5, 6]]

jumlah_array = [[0, 0], [0, 0]]
kurang_array = [[0, 0], [0, 0]]

for i in range(len(Array1)):  
    for j in range(len(Array1[i])): 
        jumlah_array[i][j] = Array1[i][j] + Array2[i][j]
        kurang_array[i][j] = Array1[i][j] - Array2[i][j]

print("Hasil Penjumlahan:",jumlah_array)
print("Hasil Pengurangan:",kurang_array)

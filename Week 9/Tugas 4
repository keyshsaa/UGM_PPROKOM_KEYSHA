def find_multiples(arr, num):
  multiples = []
  for element in arr:
    if element % num == 0:
      multiples.append(element)
  return multiples

jumlah_elemen = int(input("Masukkan jumlah elemen dalam array: "))
arr = list(range(1, jumlah_elemen + 1))
print("Array:", arr)
bilangan = int(input("Masukkan bilangan untuk mencari kelipatan: "))
hasil = find_multiples(arr, bilangan)
if hasil:
  print("Elemen yang merupakan kelipatan dari", bilangan, "adalah:", hasil)
else:
  print("Tidak ada elemen yang merupakan kelipatan dari", bilangan)

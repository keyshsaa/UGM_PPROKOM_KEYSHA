jumlah_elemen = int(input("Masukkan jumlah elemen: "))
array = []
for i in range(1, jumlah_elemen + 1):
    array.append(i)
print("Array:", array)

pembagi = int(input("Masukkan bilangan pembagi: "))
kelipatan = []
for angka in array:
    if angka % pembagi == 0:
        kelipatan.append(angka)

if kelipatan:
    print("Kelipatan dari", pembagi, "adalah:", kelipatan)
else:
    print("Tidak ada elemen yang merupakan kelipatan dari", pembagi)
